        "libs/core/api/service/src/index.ts"
      ],
      "@strawberry/core/environments/service": [
        "libs/core/environments/service/src/index.ts"
      ],
      "@strawberry/core/forms/extract": [
        "libs/core/forms/extract/src/index.ts"
      ],
      "@strawberry/core/forms/mask": [
        "libs/core/forms/mask/src/index.ts"
      ],
      "@strawberry/core/forms/utils": [
        "libs/core/forms/utils/src/index.ts"
      ],
      "@strawberry/core/hammer": [
        "libs/core/hammer/src/index.ts"
      ],
      "@strawberry/core/layout/service": [
        "libs/core/layout/service/src/index.ts"
      ],
      "@strawberry/core/navigation/common": [
        "libs/core/navigation/common/src/index.ts"
      ],
      "@strawberry/core/navigation/service": [
        "libs/core/navigation/service/src/index.ts"
      ],
      "@strawberry/core/navigation/ui/pipes": [
