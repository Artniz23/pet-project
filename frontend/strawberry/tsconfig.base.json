        "libs/core/storage/common/src/index.ts"
      ],
      "@strawberry/core/storage/local": [
        "libs/core/storage/local/src/index.ts"
      ],
      "@strawberry/core/storage/memory": [
        "libs/core/storage/memory/src/index.ts"
      ],
      "@strawberry/core/storage/session": [
        "libs/core/storage/session/src/index.ts"
      ],
      "@strawberry/core/testing": [
        "libs/core/testing/src/index.ts"
      ],
      "@strawberry/core/utils/operators": [
        "libs/core/utils/operators/src/index.ts"
      ],
      "@strawberry/core/utils/types": [
        "libs/core/utils/types/src/index.ts"
